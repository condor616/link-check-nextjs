{
  "name": "link-check",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "concurrently -n \"NEXT,WORKER\" -c \"cyan,magenta\" \"npm run dev:next\" \"npm run worker\"",
    "build": "npx prisma generate && next build",
    "build:worker": "esbuild scripts/worker.ts --bundle --platform=node --external:@prisma/client --outfile=.next/standalone/worker.js",
    "build:prod": "npm run build && npm run build:worker && cp -r public .next/standalone/public && cp -r .next/static .next/standalone/.next/static",
    "db:deploy": "prisma migrate deploy",
    "prod": "concurrently -n \"NEXT,WORKER\" -c \"cyan,magenta\" \"DATABASE_URL=file:$(pwd)/prisma/dev.db node .next/standalone/server.js\" \"DATABASE_URL=file:$(pwd)/prisma/dev.db node .next/standalone/worker.js\"",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx --watch scripts/worker.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-alert-dialog": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@supabase/supabase-js": "^2.49.4",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.9.4",
    "lucide-react": "^0.503.0",
    "next": "^16.0.6",
    "next-themes": "^0.4.6",
    "p-limit": "^6.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.6",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5"
  }
}